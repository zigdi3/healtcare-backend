{
  "name": "odapp-api",
  "version": "1.0.0",
  "description": "API Enfermagem",
  "main": "/src/main/server.ts",
  "license": "MIT",
  "engines": {
    "node": "14.17"
  },
  "scripts": {
    "start": "npm run build && cross-env && node dist/main/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --clear src/main/server.ts",
    "build": "rimraf dist/ && tsc -p tsconfig-build.json",
    "build-all": "rimraf dist/ && tsc -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js ",
    "migration:create": "npm run typeorm migration:create -- -n ",
    "migration:run": "npm run typeorm migration:run --",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm migration:revert --"
  },
  "dependencies": {
    "cross-var": "^1.1.0",
    "helmet": "^6.0.0",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "pg": "^8.8.0",
    "sqlite3": "^5.1.2",
    "tsconfig-paths": "^4.1.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-standard": "^5.0.0",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.8.4"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "private": true,
  "workspaces": {
    "nohoist": [
      "module-alias"
    ]
  }
}